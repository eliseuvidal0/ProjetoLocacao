<Window x:Class="ProjetoLocacao.View.frmConsultarLocacao"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjetoLocacao.View"
        mc:Ignorable="d"
        Title="frmConsultarLocacao" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dtaLocacao" IsEnabled="False">
            <DataGrid.Columns>
                <DataGridTextColumn x:Name="lstId" Header="Id" Binding="{Binding Id}"/>
                <DataGridTextColumn x:Name="lstCliente" Header="Cliente" Binding="{Binding Cliente}"/>
                <DataGridTextColumn x:Name="lstAgente" Header="Agente" Binding="{Binding Agente}"/>
                <DataGridTextColumn x:Name="lstVeiculo" Header="Veiculo" Binding="{Binding Veiculo}"/>
                <DataGridTextColumn x:Name="lstDevolvido" Header="Devolvido" Binding="{Binding Locacao}"/>
                <DataGridTextColumn x:Name="lstPrevisaoEntrega" Header="Previsão de Entrega" Binding="{Binding Previsao}"/>
                
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
